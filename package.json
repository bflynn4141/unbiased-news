{
  "name": "unbiased-news",
  "version": "0.1.0",
  "description": "An unbiased news aggregator that collects, analyzes, and presents news from multiple sources with bias detection",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "unbiased": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli/index.ts",
    "cli": "tsx src/cli/index.ts",
    "mcp": "tsx src/mcp/index.ts",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "news",
    "aggregator",
    "bias-detection",
    "mcp",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "commander": "^12.0.0",
    "rss-parser": "^3.13.0",
    "chalk": "^5.3.0",
    "date-fns": "^3.3.0",
    "zod": "^3.22.0",
    "p-limit": "^5.0.0",
    "natural": "^6.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
